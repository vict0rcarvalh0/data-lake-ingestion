<yandex>
  <!-- UsuÃ¡rios e perfis -->
  <users>
    <default>
      <password>admin</password>
      <networks>
        <ip>::/0</ip>
      </networks>
      <profile>default</profile>
      <quota>default</quota>
    </default>
  </users>

  <!-- Perfis -->
  <profiles>
    <default>
      <max_memory_usage>10000000000</max_memory_usage>
    </default>
  </profiles>

  <!-- Quotas -->
  <quotas>
    <default>
      <interval>
        <duration>3600</duration>
        <queries>0</queries>
        <errors>0</errors>
        <result_rows>0</result_rows>
        <read_rows>0</read_rows>
        <execution_time>0</execution_time>
      </interval>
    </default>
  </quotas>
</yandex>